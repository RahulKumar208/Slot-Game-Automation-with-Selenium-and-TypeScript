{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2022",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "paths": {
      "chrome": ["node_modules/selenium-webdriver/chrome"]
    }
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules"]
}
